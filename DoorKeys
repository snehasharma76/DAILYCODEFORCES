#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
 
    for(int i=0;i<t;i++)
    {
       
       string s;
       cin>>s;
       if (s.find('r')<s.find('R') && s.find('b')<s.find('B') && s.find('g')<s.find('G')){
           cout<<"Yes"<<endl;
       }
else cout<<"No"<<endl;
    }
}
